{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "indent": [
            true,
            "tabs"
        ],
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never"
            }
        ]
    },
    "rulesDirectory": []
}
